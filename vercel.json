{
  "rewrites": [
      { 
          "source": "/auth-callback.html", 
          "destination": "/auth-callback.html" 
      },
      { 
          "source": "/(.*)", 
          "destination": "/index.html" 
      }
  ],
  "headers": [
      {
          "source": "/(.*)",
          "headers": [
              {
                  "key": "Content-Security-Policy",
                  "value": "default-src 'self' https: 'unsafe-inline' 'unsafe-eval'; img-src 'self' https: data: blob:;"
              }
          ]
      }
  ]
}