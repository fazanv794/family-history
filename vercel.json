{
  "rewrites": [
    { "source": "/auth-callback.html", "destination": "/auth-callback.html" },
    { "source": "/chats.js", "destination": "/chats.js" },
    { "source": "/tree-builder-simple.js", "destination": "/tree-builder-simple.js" },
    { "source": "/tree-engine.js", "destination": "/tree-engine.js" },
    { "source": "/profile.js", "destination": "/profile.js" },
    { "source": "/supabase.js", "destination": "/supabase.js" },
    { "source": "/app.js", "destination": "/app.js" },
    { "source": "/style.css", "destination": "/style.css" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https:; connect-src 'self' https: wss://*.supabase.co wss://*.supabase.com ws:; img-src 'self' https: data: blob:; style-src 'self' 'unsafe-inline' https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://unpkg.com https://*.supabase.co; frame-src 'self' https:;"
        }
      ]
    }
  ]
}